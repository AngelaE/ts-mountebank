{
  "packages": [
    "docker@latest",
    "nodejs@18.19.0",
    "corepack_18@18.19.0"
  ],
  "shell": {
    "init_hook": [
      "echo 'Welcome to devbox!' > /dev/null",
      "corepack prepare pnpm@8.14.1 --activate",
      "pnpm --frozen-lockfile --strict-peer-dependencies recursive install"
    ]
  }
}
