{
  "name": "@anev/ts-mountebank-tests",
  "version": "1.0.0",
  "description": "Integration tests for @anev/ts-mountebank ",
  "engines": {
    "node": ">=16.20.2",
    "pnpm": ">=8.14.1"
  },
  "scripts": {
    "start:mb": "mb",
    "test": "mocha --recursive ./dist",
    "test-local": "pnpm mocha:test-ci",
    "test-ci": "run-p -clnr start:mb mocha:test-ci",
    "mocha:test-ci": "wait-on tcp:2525 && mocha --recursive --forbid-only ./dist",
    "build": "tsc",
    "pretty": "prettier -w ./**/*",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "mb:up": "docker run -d --name mb -p 2525:2525 -p 12345:12345 -p 12346:12346 bbyars/mountebank:2.9.1 mb start",
    "mb:up-on-port-2524": "docker run -d --name mb -p 2524:2525 -p 12345:12345 -p 12346:12346 bbyars/mountebank:2.9.1 mb start",
    "test-different-mb-url": "MB_PORT=2524 pnpm test-ci -- -g ^Mountebank",
    "mb:down": "docker kill mb && docker rm mb"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AngelaE/ts-mountebank"
  },
  "author": "Angela Evans",
  "license": "ISC",
  "dependencies": {
    "superagent": "^8.1.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/superagent": "^8.1.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "mountebank": "^2.9.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.2.1",
    "stream": "0.0.2",
    "typescript-formatter": "^7.2.2",
    "wait-on": "^7.2.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts"
}
