{
  "name": "@anev/ts-mountebank",
  "version": "1.8.2",
  "description": "Mountebank client for TS / node ",
  "engines": {
    "node": ">=16.20.2",
    "pnpm": ">=8.14.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "release": "pnpm build && pnpm test && pnpm publish --access=public",
    "package": "pnpm build && pnpm test && pnpm pack",
    "test": "mocha --recursive ./lib",
    "test-ci": "mocha --recursive --forbid-only ./lib",
    "build": "tsc",
    "pretty": "prettier -w ./**/*",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AngelaE/ts-mountebank"
  },
  "author": "Angela Evans",
  "license": "ISC",
  "dependencies": {
    "superagent": "^8.1.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/superagent": "^8.1.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.1",
    "stream": "0.0.2",
    "typescript-formatter": "^7.2.2"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts"
}
