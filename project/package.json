{
  "name": "@anev/ts-mountebank",
  "version": "1.8.2",
  "description": "Mountebank client for TS / node ",
  "engines": {
    "node": ">=20.11",
    "pnpm": ">=8.15.7"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "release": "pnpm build && pnpm test && pnpm publish --access=public",
    "package": "pnpm build && pnpm test && pnpm pack",
    "test": "mocha --recursive ./lib",
    "test-ci": "mocha --recursive --forbid-only ./lib",
    "build": "tsc",
    "pretty": "prettier -w ./**/*",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AngelaE/ts-mountebank"
  },
  "author": "Angela Evans",
  "license": "ISC",
  "dependencies": {
    "superagent": "^8.1.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/superagent": "^8.1.6",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "chai": "^4.4.1",
    "eslint": "^8.57.0",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "stream": "0.0.2",
    "typescript-formatter": "^7.2.2"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts"
}
